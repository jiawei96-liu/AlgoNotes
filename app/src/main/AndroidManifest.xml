<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.software.ustc.superspy">

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/superspy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/superspy"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppUsageAnalysisActivity"></activity>
        <activity android:name=".SysInfoShowActivity" />
        <activity android:name=".AppUsageActivity" />
        <activity android:name=".AppInfoShowActivity" />
        <activity android:name=".RollActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>